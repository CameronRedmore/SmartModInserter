<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.narrowtux.fmm.gui.SavesTabController" fx:id="root" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
    <children>
        <VBox id="VBox" alignment="CENTER" spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <padding>
                <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
            </padding>
            <children>
                <ListView fx:id="saves" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                <HBox alignment="CENTER_RIGHT" maxHeight="-1.0" prefHeight="-1.0" prefWidth="200.0" spacing="7.0" VBox.vgrow="SOMETIMES">
                    <children>
                        <Button mnemonicParsing="false" onAction="#onPlaySavePressed" text="Play" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
